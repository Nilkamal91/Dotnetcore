@using app.Models;
@model IEnumerable<app.Models.XXIBM_PRODUCT_STYLE>
@{
    ViewData["Title"] = "Style List";
}
<h2>@ViewData["Title"]</h2>
<!--<h3>@ViewData["Message"]</h3>-->

<div style="font-family:Arial">
<Table cellpadding="0" cellspacing="0">
<tr>
<th>ITEM NUMBER</th>
<th>DESCRIPTION</th>
<th>LONG_DESCRIPTION</th>
</tr>
	@foreach (XXIBM_PRODUCT_STYLE style in Model)
	{
	<tr>
		<TD>@style.Item_Number</TD>
		<TD>@style.Description</TD>
		<TD>@style.Long_Description</TD>
	</tr>
	}
</ul>
</div>
